<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">

  </head>
  <body>
    <h1>Mongoose Dashboard</h1>
    <div>
        <ul>
          <% for(var i = animals.length -1; i > -1; i--) {%>
              <li><strong>Name:</strong> <a href="mongooses/<%= animals[i]._id %>"><%= animals[i].name %></a></li>
          <% } %>
        </ul>
      </div>

      <p><a href="mongooses/new">Add New Animal</a></p>

  </body>
</html>
